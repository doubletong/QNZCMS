@model IEnumerable<QNZ.Data.Navigation>
@{
    var url = Context.Request.Path.ToString();
}

    <div class="submenus">
        @foreach (var item in Model)
        {
            <a class="@((url==item.Url || url.Contains(item.Url)) ?"active":"")" href="@item.Url" @(item.MenuType == (short)QNZ.Data.Enums.MenuType.OUTLINK ?"target='_blank'":"")>@item.Title</a>
        }     
    </div>

