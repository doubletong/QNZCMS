<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@($"{ViewBag.Title}_{SettingsManager.Site.SiteName}")</title>
    <meta name="description" content="">
    <meta name="robots" content="all">
    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="format-detection" content="telephone=no">
    <meta name="format-detection" content="email=no">

    <link rel="manifest" href="site.webmanifest">
    <link rel="apple-touch-icon" href="icon.png">

    <link href="~/assets/fonts/iconfont.css" rel="stylesheet" />
    <link href="~/plugins/wow/css/libs/animate.css" rel="stylesheet" />
    <link href="~/assets/css/styles.min.css" rel="stylesheet" />
    <script src="~/plugins/wow/dist/wow.js"></script>
    <script>
        new WOW().init();


    </script>
    @RenderSection("Header", required: false)
</head>
<body>
    <!--[if lte IE 9]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
    <![endif]-->
    @await Html.PartialAsync("_Header")

    @RenderBody()

    @await Html.PartialAsync("_Footer")


    <script src="~/assets/js/libs/jquery-1.12.4.min.js"></script>
    <script src="~/assets/js/app.js"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>
